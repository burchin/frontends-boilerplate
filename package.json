{
  "name": "frontends-boilerplate",
  "version": "1.0.8",
  "description": "前端开发脚手架",
  "author": "ZG FED",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/ziguang_burchin/frontends-boilerplate.git"
  },
  "pre-commit": [
    "lint"
  ],
  "scripts": {
    "lint": "node lint.js",
    "lint:fix": "npm run lint --fix",
    "clean:lib": "rimraf ./lib",
    "clean:dist": "rimraf ./dist",
    "lib": "webpack --progress --config ./webpack/webpack.lib.conf.js -p",
    "build": "npm run clean:dist && webpack --env=prod",
    "build:anal": "npm run clean:dist && webpack --anal=1",
    "build:dev": "npm run clean:dist && webpack --env=dev",
    "build:test": "npm run clean:dist && webpack --env=test",
    "build:mock": "npm run clean:dist && webpack --env=mock",
    "mock": "webpack-dev-server --env=mock",
    "dev": "webpack-dev-server --env=dev",
    "pro": "webpack-dev-server --env=prod"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "add-asset-html-webpack-plugin": "^2.1.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "^1.11.0",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.0",
    "cssnano": "^3.10.0",
    "eslint": "^5.3.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "node-sass": "^4.9.3",
    "postcss-aspect-ratio-mini": "0.0.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "2.0.8",
    "postcss-px-to-viewport": "0.0.3",
    "postcss-viewport-units": "^0.1.4",
    "postcss-write-svg": "^3.0.1",
    "pre-commit": "^1.2.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "ts-import-plugin": "^1.5.5",
    "typescript": "^3.5.1",
    "typescript-eslint-parser": "^22.0.0",
    "url-loader": "^1.1.0",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-parallel-uglify-plugin": "^1.1.0"
  },
  "dependencies": {
    "antd-mobile": "^2.2.14",
    "axios": "^0.18.0",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.4",
    "react-router-dom": "^5.0.1"
  }
}
